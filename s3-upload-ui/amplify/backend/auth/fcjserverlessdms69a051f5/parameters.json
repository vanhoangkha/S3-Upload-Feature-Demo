{
    "authRoleArn": {
        "Fn::GetAtt": ["AuthRole", "Arn"]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": ["UnauthRole", "Arn"]
    },
    "authRoleName": {
        "Ref": "AuthRole"
    },
    "unauthRoleName": {
        "Ref": "UnauthRole"
    },
    "userPoolName": "vibdmsstore2026-userpool",
    "autoVerifiedAttributes": ["email"],
    "mfaConfiguration": "OFF",
    "mfaTypes": ["SMS"],
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": ["REQUIRES_LOWERCASE", "REQUIRES_UPPERCASE", "REQUIRES_NUMBERS", "REQUIRES_SYMBOLS"],
    "requiredAttributes": ["email"],
    "userpoolClientGenerateSecret": false,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": ["email"],
    "userpoolClientReadAttributes": ["email"],
    "userpoolClientLambdaRole": "vibdmsstore2026-userpool-client-lambda-role",
    "userpoolClientSetAttributes": false,
    "sharedId": "vibdmsstore2026",
    "resourceName": "vibdmsstore2026-userpool",
    "authSelections": "identityPoolAndUserPool",
    "useDefault": "default",
    "usernameAttributes": ["email"],
    "userPoolGroupList": ["admin", "vendor"],
    "serviceName": "Cognito",
    "usernameCaseSensitive": false,
    "useEnabledMfas": true,
    "breakCircularDependency": true,
    "dependsOn": [],
    "allowUnauthenticatedIdentities": false,
    "identityPoolName": "vibdmsstore2026-identitypool"
} 